

<div class="upload">
	<% path= "#{ENV['RAILS_RELATIVE_URL_ROOT']}#{upload.arquivo.url}" %>

	<%= "#{upload.user.razao_social}" if current_user.admin? %>

		<div class="edit_icon">
	<%= link_to image_tag("icons/pencil.png",:alt=>"Alterar proposta") ,  edit_edital_upload_path(@edital,upload), :remote=>true unless current_user.admin?%>
		</div>
		
		<span class="timestamp">
			<%=  link_to upload.arquivo_file_name,path %> postado a  <%= time_ago_in_words(upload.updated_at) %>
		</span>

	<div class="clear"></div>

</div>




